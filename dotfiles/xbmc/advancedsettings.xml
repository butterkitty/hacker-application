	<regexp>[0]*([0-9]+)x[0]*([0-9]+)[^\\/]*</regexp>
	<regexp>[\\/]s([0-9]+)e([0-9]+)</regexp>
	<regexp>Season ([0-9]+) - Episode ([0-9]+)[^\\/]*</regexp>
